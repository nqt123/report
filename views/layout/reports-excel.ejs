<%- breadcrumb %>
<div class="card">
    <div class="portlet-title">
        <div class="lv-header-alt clearfix">
            <h2 class="lvh-label hidden-xs"><%= title %></h2>
            <ul class="lv-actions actions">
                <li><a data-toggle="tooltip" data-placement="top"
                       data-original-title="Refresh"><i
                                class="zmdi zmdi-refresh"></i></a></li>
            </ul>
        </div>
        <div class="clearfix"></div>
        <form class="form-horizontal" role="form">
            <div class="form-body">
                <div class="row m-0 m-t-20">
                    <div class="col-md-4 m-b-20">
                        <label for="startDate"
                               class="control-label f-13"><span>Thời gian gửi yêu cầu</span></label>

                        <div class="input-group">
                            <input type="text"
                                   class="form-control pull-right search-field date-picker filter-campain input"
                                   id="startDate" name="startDate"
                                   placeholder="Ngày bắt đầu, VD: 01/01/2016">
                            <span class="input-group-addon p-l-10 bgm-gray c-white"><i role="button"
                                                                                       class="zmdi zmdi-calendar"></i></span>
                        </div>
                    </div>
                    <div class="col-md-4 m-b-20">
                        <label for="endDate"
                               class="control-label f-13"><span>Thời gian kết thúc yêu cầu</span></label>

                        <div class="input-group">
                            <input type="text"
                                   class="form-control pull-right search-field date-picker filter-campain input"
                                   id="endDate" name="endDate"
                                   placeholder="Ngày kết thúc, VD: 31/12/2016">
                            <span class="input-group-addon p-l-10 bgm-gray c-white"><i role="button"
                                                                                       class="zmdi zmdi-calendar"></i></span>
                        </div>
                    </div>

                    <div class="col-md-4 m-b-20" style="z-index: 8">
                        <label for="companies"
                               class="control-label f-13"><span>Trường thông tin</span></label>
                        <select name="companies" multiple class="selectpicker input" title="Chọn trường thông tin" multiple >
                          <option value="" id="all">Tất cả</option>
                          <option value="id">ID</option>
                          <option value="displayName">Tên Dự Án</option>
                          <option value="usingCRM">CRM đang sử dụng</option>
                          <option value="slamenus.displayName">Loại sự cố</option>
                          <option value="slalists.name">Dạng sự cố</option>
                          <option value="">SLA</option>
                          <option value="title">Tiêu đề sự cố</option>
                          <option value="description">Nội dung sự cố</option>
                          <option value="agentNumberInShift">Số lượng nhân sự trong ca</option>
                          <option value="agentNumberInfluence">Số lượng nhân sự ảnh hưởng</option>
                          <option value="percentOfInfluence">Mức độ ảnh hương</option>
                          <option value="prior">Mức độ ưu tiên xử lý</option>
                          <option value="createdBy">Người gửi yêu cầu</option>
                          <option value="createdAt">Thời gian bắt đầu</option>
                          <option value="typeOfCause">Nguyên nhân</option>
                          <option value="detailCause">Chi tiết nguyên nhân</option>
                          <option value="contentHandle">Nội dung xử lý</option>
                          <option value="statusAfterHandle">Tình trạng sau xử lý</option>
                          <option value="offerSolution">Giải pháp</option>
                          <option value="statusAfterHandle">Trạng thái xử lý</option>
                          <option value="supporter">Kỹ thuật xử lý </option>
                          <option value="lastRespondAt">Thời gian kết thúc</option>
                          <option value="totalTime">Tổng thời gian xử lý</option>
                          <option value="state">Trạng thái yêu cầu</option>
                        </select>
                    </div>

                </div>
                <div class="row m-0">
                    <div class="col-md-3"></div>
                    <div class="col-md-6 m-b-20">
                        <div class="btn-group btn-group btn-group-justified">
                            <a type="submit" class="btn bgm-blue uppercase c-white" style="width:auto"><i
                                        class="fa fa-search m-r-10"></i><strong>Tìm
                                    kiếm</strong></a>

                            <div class="col-md-2"></div>
                            <a id="exportexcel" class="btn bgm-blue default c-white" style="width:auto"><i
                                        class="fa fa-file-excel-o m-r-10"></i>Xuất Excel</a>
                        </div>
                    </div>
                    <div class="col-md-3"></div>
                </div>
            </div>
        </form>
    </div>



    <div class="card-body table-responsive" style=" min-height:200px">
        <table class="table table-hover table-condensed table-bordered table-fix" style="table-layout: fixed;"
               id="exceldata">
            <thead>
            <tr>
                <td class="bgm-orange c-white text-center" colspan="3">Outbound</td>
                <td class="bgm-orange c-white text-center" colspan="4">Inbound</td>
            </tr>
            <tr>

                <td class="bgm-orange c-white text-center"><span>Tổng gọi ra</span></td>
                <td class="bgm-orange c-white text-center"><span>SL cuộc gọi kết nối</span></td>
                <td class="bgm-orange c-white text-center"><span>Tổng số phiếu</span></td>
                <td class="bgm-orange c-white text-center"><span>Tổng gọi vào</span></td>
                <td class="bgm-orange c-white text-center"><span>SL cuộc gọi được phục vụ</span></td>
                <td class="bgm-orange c-white text-center"><span>SL cuộc gọi nhỡ</span></td>
                <td class="bgm-orange c-white text-center"><span>Tổng số phiếu</span></td>

            </tr>
            </thead>
            <tbody id="tbBody">

            </tbody>
        </table>
        <div id="container">
        </div>
    </div>
</div>
