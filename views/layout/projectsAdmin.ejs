<%- breadcrumb %>

<div class="card">
  <div class="lv-header-alt clearfix m-b-5">
    <h2 class="lvh-label hidden-xs"><%- title %></h2>
    <ul class="lv-actions actions">
      <li>
        <a href="/#projectsAdmin/new" data-toggle="tooltip" data-placement="top" data-original-title="Thêm Yêu cầu mới">
          <i class="zmdi zmdi-plus"></i>
        </a>
      </li>

      <li>
        <a href="#" data-toggle="tooltip" data-placement="top" data-original-title="Làm mới danh sách" id="refreshBtn">
          <i class="zmdi zmdi-refresh"></i>
        </a>
      </li>
      <li class="hidden" id="li-hidden">
        <a role="button" data-toggle="tooltip" data-placement="top" data-original-title="Xóa các mục đánh dấu"
          id="btn-delSelection">
          <i class="zmdi zmdi-delete"></i>
        </a>
      </li>
    </ul>
  </div>
  <div class="">
    <div>
      <div class="panel-body p-0">
        <div class="tab-content">
          <div role="tabpanel" style="height: 100%" class="tab-pane animated active" id="tab-not-process-ticket">
            <form role="form" id="project">
              <table class="table table-bordered table-fix table-hover" id="tbl-ticket" style="overflow:auto">
                <thead>
                  <tr class="listHead">
                    <th data-sort="none" role="button" class="col-lg-3 bgm-orange c-white text-center"
                      style="width: 5%;">
                      TT
                    </th>
                    <th data-sort="none" data-field="name" role="button" class="bgm-orange c-white text-center">
                      <i data-toggle="tooltip" data-original-title="Sắp xếp theo thứ tự tăng dần"
                        class="zmdi zmdi-sort-asc"></i>
                      <i data-toggle="tooltip" data-original-title="Sắp xếp theo thứ tự giảm dần"
                        class="zmdi zmdi-sort-desc"></i>
                      Tên dự án
                    </th>
                    <th data-sort="none" sortName="name" role="button" class="bgm-orange c-white text-center sort">
                      Vị trí dự án
                    </th>
                    <th data-sort="none" sortName="name" role="button" class="bgm-orange c-white text-center sort">
                      CRM Đang sử dụng
                    </th>
                    <th data-sort="none" sortName="name" role="button" class="bgm-orange c-white text-center sort">
                      Thời Gian GOLINE
                    </th>
                    <th data-sort="none" data-field="offTime" role="button" class="bgm-orange c-white text-center">
                      <i data-toggle="tooltip" data-original-title="Sắp xếp theo thứ tự tăng dần"
                        class="zmdi zmdi-sort-asc"></i>
                      <i data-toggle="tooltip" data-original-title="Sắp xếp theo thứ tự giảm dần"
                        class="zmdi zmdi-sort-desc"></i>
                      thời gian off
                    </th>
                    <!-- <th class="bgm-orange c-white text-center">
                      Tình trạng
                    </th> -->
                    <th class="bgm-orange c-white text-center">
                      CRM
                    </th>
                    <th class="bgm-orange c-white text-center">
                      Voice
                    </th>
                    <th class="bgm-orange c-white text-center">
                      Chat
                    </th>
                    <th class="bgm-orange c-white text-center">
                      Email
                    </th>
                    <th class="bgm-orange c-white text-center">
                      Smart IVR
                    </th>
                    <th class="bgm-orange c-white text-center">
                      SMS
                    </th>
                    <th class="bgm-orange c-white text-center">
                      Phần cứng
                    </th>
                    <th class="bgm-orange c-white text-center">
                      Đường truyền
                    </th>
                    <th class="bgm-orange c-white text-center">
                      IP
                    </th>
                    <th role="button" data-field="agentNumber" data-sort="none" class="bgm-orange c-white text-center">
                      <i data-toggle="tooltip" data-original-title="Sắp xếp theo thứ tự tăng dần"
                        class="zmdi zmdi-sort-asc"></i>
                      <i data-toggle="tooltip" data-original-title="Sắp xếp theo thứ tự giảm dần"
                        class="zmdi zmdi-sort-desc"></i>
                      Số lượng Agent
                    </th>
                    <th class="bgm-orange c-white text-center">
                      Thao tác
                    </th>
                  </tr>
                </thead>
                <tbody class="report-list_body">
                  <tr id="searchForm" class="filter">
                    <td class="text-center"></td>
                    <td class="text-center"><input class="form-control search" type="text" name="filter_name"></td>
                    <td class="text-center"><input class="form-control search" type="text" name="filter_position"></td>
                    <td class="text-center"><input class="form-control search" type="text" name="filter_usingCRM"></td>
                    <td class="text-center"><input class="form-control search" type="text" name="filter_goLineTime"></td>
                    <td class="text-center"><input class="form-control search" type="text" name="filter_offTime"></td>
                    <td class="text-center"><input type="checkbox" class="search" name="filter_CRM"></td>
                    <td class="text-center"><input type="checkbox" class="search" name="filter_Voice"></td>
                    <td class="text-center"><input type="checkbox" class="search" name="filter_Chat"></td>
                    <td class="text-center"><input type="checkbox" class="search" name="filter_Email"></td>
                    <td class="text-center"><input type="checkbox" class="search" name="filter_smartIVR"></td>
                    <td class="text-center"><input type="checkbox" class="search" name="filter_SMS"></td>
                    <td class="text-center"><input type="checkbox" class="search" name="filter_hardware"></td>
                    <td class="text-center"><input type="checkbox" class="search" name="filter_connection"></td>
                    <td class="text-center"><input class="form-control search" type="text" name="filter_IP"></td>
                    <td class="text-center"><input class="form-control search" type="text" name="filter_agentNumber">
                    </td>
                    <td class="text-center"><button class="btn btn-primary btn-block waves-effect" id="btn-search"
                        name="btn-search"><i class="zmdi zmdi-filter-list m-r-5"></i> Lọc</button></td>
                  </tr>
                  <%result.forEach((e,i) => {%>
                  <tr id="item">
                    <td class="text-center">
                      <%=i+1%>
                    </td>
                    <td id="id" hidden>
                      <%=e._id%>
                    </td>
                    <td id="id">
                      <%=e.name%>
                    </td>
                    <td id="id">
                      <%=e.position%>
                    </td>
                    <td id="id">
                      <%=e.usingCRM%>
                    </td>
                    <td id="id">
                      <%=e.goLineTime%>
                    </td>
                    <td>
                      <%=e.offTime%>
                    </td>
                    <td class="text-center">
                      <%if(e.CRM){%>
                      <input type="checkbox" checked>
                      <%}%>
                    </td>
                    <td class="text-center">
                        <%if(e.Voice){%>
                      <input type="checkbox" checked>
                      <%}%>
                    </td>
                    <td class="text-center">
                        <%if(e.Chat){%>
                      <input type="checkbox" checked>
                      <%}%>
                    </td>
                    <td class="text-center">
                        <%if(e.Email){%>
                      <input type="checkbox" checked>
                      <%}%>
                    </td>
                    <td class="text-center">
                        <%if(e.smartIVR){%>
                      <input type="checkbox" checked>
                      <%}%>
                    </td>
                    <td class="text-center">
                        <%if(e.SMS){%>
                      <input type="checkbox" checked>
                      <%}%>
                    </td>
                    <td class="text-center">
                        <%if(e.hardware){%>
                      <input type="checkbox" checked>
                      <%}%>
                    </td>
                    <td class="text-center">
                        <%if(e.connection){%>
                      <input type="checkbox" checked>
                      <%}%>
                    </td>
                    <td class="text-center">
                        <%=e.IP%>
                    </td>
                    <td class="text-center">
                      <%=e.agentNumber%>
                    </td>
                    <td class="text-center" style="display: flex; justify-content:space-evenly;" role="button">
                      <div data-toggle="tooltip" data-placement="top" data-original-title="Xoá"> <i id="delete"
                          class="zmdi zmdi-delete text-danger" style="font-size: 2rem;"></i></div>
                      <div data-toggle="tooltip" data-placement="top" data-original-title="Cập nhật"> <i id="update"
                          class="zmdi zmdi-edit text-info" style="font-size: 2rem;"></i></div>
                    </td>
                  </tr>
                  <%});%>
                </tbody>
              </table>
              <div class="text-center" id="ticket-total">
                <!--<b>-->
                <!--<span class="TXT_TOTAL"></span>:-->
                <!--<span class="bold c-red" id="ticket-total">100</span>-->
                <!--</b>-->
              </div>
              <div class="text-center" id="ticket-paging"></div>
            </form>
          </div>
        </div>
      </div>
    </div>
    <% if(paging.pageCount > 1){ %>
    <% include ../paging.ejs %>
    <% } %>
  </div>
</div>
<script>

</script>